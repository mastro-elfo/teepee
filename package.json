{
  "name": "teepee",
  "productName": "Teepee",
  "version": "1.3.0",
  "description": "A simple management application",
  "main": ".webpack/main",
  "homepage": "https://github.com/mastro-elfo/teepee#README",
  "bugs": {
    "url": "https://github.com/mastro-elfo/teepee/issues"
  },
  "keywords": [
    "react",
    "reactjs",
    "electron",
    "electro-app",
    "webpack",
    "material-ui"
  ],
  "author": {
    "name": "mastro-elfo",
    "email": "francesco.209@gmail.com",
    "url": "https://mastro-elfo.github.io/"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mastro-elfo/teepee.git"
  },
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "premake": "node ./scripts/premake.js",
    "make:deb": "yarn run make --targets @electron-forge/maker-deb",
    "premake:deb": "node ./scripts/premakedeb.js",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\""
  },
  "config": {
    "forge": {
      "packagerConfig": {
        "executableName": "teepee"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "teepee"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {
            "categories": [
              "Office"
            ],
            "description": "A simple management app for non-profit organizations",
            "genericName": "Management",
            "homepage": "https://github.com/mastro-elfo/teepee",
            "icon": "./src/assets/logo.png",
            "maintainer": "Francesco Michienzi",
            "name": "teepee",
            "productDescription": "A simple management app for non-profit organizations",
            "productName": "Teepee",
            "version": "1.3.0"
          }
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/index.html",
                  "js": "./src/renderer.js",
                  "name": "main_window"
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "@electron-forge/cli": "6.0.0-beta.52",
    "@electron-forge/maker-deb": "6.0.0-beta.52",
    "@electron-forge/maker-rpm": "6.0.0-beta.52",
    "@electron-forge/maker-squirrel": "6.0.0-beta.52",
    "@electron-forge/maker-zip": "6.0.0-beta.52",
    "@electron-forge/plugin-webpack": "6.0.0-beta.52",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "babel-loader": "^8.1.0",
    "css-loader": "^3.0.0",
    "electron": "10.1.3",
    "electron-icon-maker": "^0.0.5",
    "file-loader": "^6.1.1",
    "node-loader": "^0.6.0",
    "style-loader": "^0.23.1",
    "svg-inline-loader": "^0.8.2"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "classnames": "^2.2.6",
    "electron-fs-extra": "^1.0.0",
    "electron-squirrel-startup": "^1.0.0",
    "fs-extra": "^9.0.1",
    "i18next": "^19.8.3",
    "i18next-electron-language-detector": "^0.0.10",
    "lodash": "^4.17.20",
    "mastro-elfo-mui": "^1.16.0",
    "node-json-db": "^1.1.0",
    "notistack": "^1.0.1",
    "prefix-si": "^1.2.0",
    "prop-types": "^15.7.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-hot-loader": "^4.13.0",
    "react-i18next": "^11.7.3",
    "react-markdown": "^4.3.1",
    "react-router-dom": "^5.2.0",
    "react-smooth-dnd": "^0.11.1"
  }
}
