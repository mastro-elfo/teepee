{
  "name": "teepee",
  "productName": "teepee",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "prestart": "yarn run init",
    "package": "electron-forge package",
    "init": "node ./scripts/init.js",
    "make": "electron-forge make",
    "make:deb": "electron-forge make --targets @electron-forge/maker-deb",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\""
  },
  "keywords": [],
  "author": {
    "name": "mastro-elfo",
    "email": "francesco.209@gmail.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "teepee"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/index.html",
                  "js": "./src/renderer.js",
                  "name": "main_window"
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "@electron-forge/cli": "6.0.0-beta.52",
    "@electron-forge/maker-deb": "6.0.0-beta.52",
    "@electron-forge/maker-rpm": "6.0.0-beta.52",
    "@electron-forge/maker-squirrel": "6.0.0-beta.52",
    "@electron-forge/maker-zip": "6.0.0-beta.52",
    "@electron-forge/plugin-webpack": "6.0.0-beta.52",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "babel-loader": "^8.1.0",
    "css-loader": "^3.0.0",
    "electron": "10.1.3",
    "electron-icon-maker": "^0.0.5",
    "file-loader": "^6.1.1",
    "node-loader": "^0.6.0",
    "style-loader": "^0.23.1",
    "svg-inline-loader": "^0.8.2"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "classnames": "^2.2.6",
    "electron-fs-extra": "^1.0.0",
    "electron-squirrel-startup": "^1.0.0",
    "fs-extra": "^9.0.1",
    "lodash": "^4.17.20",
    "mastro-elfo-mui": "^1.16.0",
    "node-json-db": "^1.1.0",
    "notistack": "^1.0.1",
    "prefix-si": "^1.2.0",
    "prop-types": "^15.7.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-hot-loader": "^4.13.0",
    "react-markdown": "^4.3.1",
    "react-router-dom": "^5.2.0",
    "react-smooth-dnd": "^0.11.1"
  }
}
